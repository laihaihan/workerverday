<?xml version='1.0' encoding='UTF-8'?>
<settings>
  <font>微软雅黑</font>
  <background>
    <alpha>100</alpha>
  </background>
  <plot_area>
    <margins>
      <top>10</top>
      <left>30</left>
      <right>50</right>
    </margins>
  </plot_area>
  <grid>
    <category>
      <dashed>1</dashed>
    </category>
    <value>
      <dashed>1</dashed>
    </value>
  </grid>
  <axes>
    <category>
      <width>1</width>
    </category>
    <value>
      <width>1</width>
    </value>
  </axes>
  <values>
    <category>
      <text_size>11</text_size>
    </category>
    <value>
      <min>0</min>
    </value>
  </values>
  <depth>15</depth>
  <column>
    <width>85</width>
    <balloon_text>{series} : {value} 件</balloon_text>
    <grow_time>3</grow_time>
  </column>
  <graphs>
    <graph gid="0">
      <title>Stock</title>
      <type>line</type>
      <color>ADD981</color>
    </graph>
    <graph gid="1">
      <title>监察数量</title>
      <color>7F8DA9</color>
    </graph>
    <graph gid="2">
      <title>Line</title>
      <color>FEC514</color>
    </graph>
  </graphs>
  <labels>
    <label lid="0">
      <text> </text>
      <y>18</y>
      <text_color>000000</text_color>
      <text_size>13</text_size>
      <align>center</align>
    </label>
  </labels>
</settings>