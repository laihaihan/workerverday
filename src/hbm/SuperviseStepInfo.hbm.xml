<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.linewell.ess.fds.stepinfo.SuperviseStepInfo" table="SUPERVISE_STEPINFO">
<id column="UNID" name="unid" type="string">
<generator class="assigned"/>
</id>
<property column="CASESN" name="casesn" type="string"/>
<property column="STEPNAME" name="stepname" type="string"/>
<property column="STARTTIME" name="starttime" type="string"/>
<property column="ENDTIME" name="endtime" type="string"/>
<property column="HANDLEPEOPLE" name="handlepeople" type="string"/>
<property column="OPINION" name="opinion" type="string"/>
<property column="CASECONTENT" name="casecontentclob" type="clob" lazy="true"/>
<property column="STEPRESULT" name="stepresult" type="string"/>
<property column="POWERID" name="powerid" type="string"/>
<property column="PUNISHLAW" name="punishlaw" type="string"/>
<property column="BREACHLAW" name="breachlaw" type="string"/>
<property column="STANDARDNAME" name="standardname" type="string"/>
<property column="PUNISHRESULT" name="punishresult" type="string"/>
<property column="PUNISHMONEY" name="punishmoney" type="long"/>
<property column="ASKRECORD" name="askrecordclob" type="clob" lazy="true"/>
<property column="CREATETIME" name="createtime" type="string"/>
<property column="ISNEW" name="isnew" type="string"/>
<property column="ISMONITOR" name="ismonitor" type="string"/>
</class>
</hibernate-mapping>
